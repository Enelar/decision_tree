<b><%= this.label %></b>
<%= this.DeferRender('submenu', {}) %>
<%
this.Defer(function()
{
  this.first()    
    .parent()
    .draggable
    ({
      drag : function(event, ui)
      {
        UpdateLinks(ui.helper);
      }
    })
    .uniqueId()
    .css(
    {
      width: 'initial', 
      'background-color': 'gray'
    });
});
%>