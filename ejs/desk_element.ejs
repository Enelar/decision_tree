<b><%= this.label %></b><br>
<%= this.DeferRender('submenu', {}) %>
<%
this.Defer(function()
{
  this.first()    
    .parent()
    .draggable
    ({
      drag : function(event, ui)
      {
        UpdateLinks(ui.helper);
      }
    })
    .uniqueId()
    .css(
    {
      width: 'initial', 
      'background-color': '#DDD',
      'padding-top': '0px',
    });
});
%>